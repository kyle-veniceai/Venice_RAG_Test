# Pinecone API Key for vector database
PINECONE_API_KEY=your_pinecone_api_key_here

# Venice.ai API Key for embeddings and LLM
VENICE_API_KEY=your_venice_api_key_here
VENICE_API_URL="https://api.venice.ai/api/v1/chat/completions"

# Embedding API Key
EMBEDDING_API_KEY=your_embedding_api_key_here
EMBEDDING_API_URL="https://api.venice.ai/api/v1/embeddings"

# Vector DB Info
VECTOR_DIMENSION= 1024  # Adjust based on your embedding model's output dimension
CHUNK_SIZE= 256  # Number of tokens/characters per chunk
CHUNK_OVERLAP= 50  # Overlap between chunks
TOP_K_RESULTS= 5  # Number of similar documents to retrieve
dimensions=1024
metric="cosine"
spec= ServerlessSpec('aws', 'us-east-1')
index_name= 'indexname'

# Optional: Port configuration (default is 8080)
PORT=8080